                </DataTemplate>
                <DataTemplate x:Key="{DataTemplateKey {x:Type model:Premium}}" DataType="{x:Type model:Premium}">
                  <views:PremiumPage />
                </DataTemplate>
                <DataTemplate x:Key="{DataTemplateKey {x:Type model:Security}}" DataType="{x:Type model:Security}">
