                <DataTemplate x:Key="{DataTemplateKey {x:Type model:Auth}}" DataType="{x:Type model:Auth}">
                  <views:AuthPage />
                </DataTemplate>
                <DataTemplate x:Key="{DataTemplateKey {x:Type model:Settings}}" DataType="{x:Type model:Settings}">
