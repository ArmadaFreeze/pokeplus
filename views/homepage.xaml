<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="PokeMMO_.Views.HomePage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:model="clr-namespace:PokeMMO_.Model" xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" xmlns:converter="clr-namespace:PokeMMO_.Converter" xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" xmlns:Themes="clr-namespace:Xceed.Wpf.Toolkit.Themes;assembly=Xceed.Wpf.Toolkit" x:Name="MyHomePage" xmlns:views="clr-namespace:PokeMMO_.Views">
  <FrameworkElement.Resources>
    <ResourceDictionary>
      <BooleanToVisibilityConverter x:Key="BoolToVis" />
      <Style x:Key="CheckComboBoxStyle1" xmlns:toolkit="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit" TargetType="{x:Type toolkit:CheckComboBox}">
        <Setter Property="Control.Background" Value="{DynamicResource MahApps.Brushes.Control.Background}" />
        <Setter Property="Control.BorderBrush" Value="{DynamicResource MahApps.Brushes.TextBox.Border}" />
        <Setter Property="Control.BorderThickness" Value="{DynamicResource ComboBoxBorderThemeThickness}" />
        <Setter Property="Control.FontFamily" Value="{DynamicResource MahApps.Fonts.Family.Control}" />
        <Setter Property="Control.FontSize" Value="{DynamicResource MahApps.Font.Size.Content}" />
        <Setter Property="Control.Foreground" Value="{DynamicResource MahApps.Brushes.Text}" />
        <Setter Property="Control.HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="Control.VerticalContentAlignment" Value="Center" />
        <Setter Property="FrameworkElement.MinHeight" Value="26" />
        <Setter Property="Control.Padding" Value="4" />
        <Setter Property="RenderOptions.ClearTypeHint" Value="Enabled" />
        <Setter Property="ScrollViewer.CanContentScroll" Value="True" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="UIElement.SnapsToDevicePixels" Value="True" />
        <Setter Property="ScrollViewer.PanningMode" Value="Both" />
        <Setter Property="Stylus.IsFlicksEnabled" Value="False" />
        <Setter Property="Control.Template">
          <Setter.Value>
            <ControlTemplate TargetType="{x:Type toolkit:CheckComboBox}">
              <Grid Name="MainGrid" SnapsToDevicePixels="True">
                <Popup Name="PART_Popup" AllowsTransparency="True" Margin="1" PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}" Placement="Bottom" StaysOpen="False" IsOpen="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                  <Grid MinWidth="{Binding ActualWidth, ElementName=MainGrid}">
                    <Border Name="DropDownBorder" BorderBrush="{DynamicResource {x:Static SystemColors.WindowFrameBrushKey}}" BorderThickness="1" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}" MaxHeight="{Binding MaxDropDownHeight, RelativeSource={RelativeSource TemplatedParent}}">
                      <ScrollViewer Name="DropDownScrollViewer">
                        <Grid RenderOptions.ClearTypeHint="Enabled">
                          <Canvas HorizontalAlignment="Left" Height="0" VerticalAlignment="Top" Width="0">
                            <Rectangle Name="OpaqueRect" Fill="{Binding Background, ElementName=DropDownBorder}" Height="{Binding ActualHeight, ElementName=DropDownBorder}" Width="{Binding ActualWidth, ElementName=DropDownBorder}" />
                          </Canvas>
                          <StackPanel>
                            <primitives:SelectAllSelectorItem xmlns:primitives="clr-namespace:Xceed.Wpf.Toolkit.Primitives;assembly=Xceed.Wpf.Toolkit" primitives:SelectAllSelectorItem.Name="PART_SelectAllSelectorItem" ContentControl.Content="{TemplateBinding SelectAllContent}">
                              <UIElement.Visibility>
                                <Binding Path="IsSelectAllActive" RelativeSource="{RelativeSource TemplatedParent}">
                                  <Binding.Converter>
                                    <BooleanToVisibilityConverter />
                                  </Binding.Converter>
                                </Binding>
                              </UIElement.Visibility>
                            </primitives:SelectAllSelectorItem>
                            <ItemsPresenter Name="PART_ItemsPresenter" KeyboardNavigation.DirectionalNavigation="Contained" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                          </StackPanel>
                        </Grid>
                      </ScrollViewer>
                    </Border>
                  </Grid>
                </Popup>
                <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" />
                <ToggleButton Name="PART_DropDownButton" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" Content="Catch Moves" Focusable="False" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" Padding="{TemplateBinding Padding}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                  <UIElement.IsHitTestVisible>
                    <Binding Path="IsDropDownOpen" RelativeSource="{RelativeSource TemplatedParent}">
                      <Binding.Converter>
                        <converters:InverseBoolConverter xmlns:converters="clr-namespace:Xceed.Wpf.Toolkit.Core.Converters;assembly=Xceed.Wpf.Toolkit" />
                      </Binding.Converter>
                    </Binding>
                  </UIElement.IsHitTestVisible>
                  <FrameworkElement.Style>
                    <Style TargetType="{x:Type ToggleButton}">
                      <Setter Property="FrameworkElement.OverridesDefaultStyle" Value="True" />
                      <Setter Property="IsTabStop" Value="False" />
                      <Setter Property="Focusable" Value="False" />
                      <Setter Property="Padding" Value="2" />
                      <Setter Property="Template">
                        <Setter.Value>
                          <ControlTemplate TargetType="{x:Type ToggleButton}">
                            <chromes:ButtonChrome xmlns:chromes="clr-namespace:Xceed.Wpf.Toolkit.Chromes;assembly=Xceed.Wpf.Toolkit" chromes:ButtonChrome.Name="Chrome" Control.BorderBrush="{TemplateBinding BorderBrush}" Control.Background="{TemplateBinding Background}" chromes:ButtonChrome.CornerRadius="0" chromes:ButtonChrome.RenderNormal="False" chromes:ButtonChrome.RenderEnabled="{TemplateBinding IsEnabled}" UIElement.SnapsToDevicePixels="True" chromes:ButtonChrome.RenderMouseOver="{Binding IsMouseOver, ElementName=PART_DropDownButton}" chromes:ButtonChrome.RenderPressed="{Binding IsPressed, ElementName=PART_DropDownButton}">
                              <Grid>
                                <Grid.ColumnDefinitions>
                                  <ColumnDefinition Width="*" />
                                  <ColumnDefinition MinWidth="{DynamicResource {x:Static SystemParameters.VerticalScrollBarWidthKey}}" Width="0" />
                                </Grid.ColumnDefinitions>
                                <TextBox Name="TextBox" BorderThickness="0" Background="#00FFFFFF" Foreground="#FFFFFFFF" Cursor="Arrow" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" HorizontalScrollBarVisibility="Hidden" IsTabStop="{TemplateBinding IsTabStop}" IsReadOnly="True" Margin="{TemplateBinding Padding}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Text="Catch Moves" VerticalScrollBarVisibility="Hidden" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                                  <FrameworkElement.Style>
                                    <Style TargetType="{x:Type TextBox}">
                                      <Style.Triggers>
                                        <Trigger Property="IsEnabled" Value="False">
                                          <Setter Property="Background" Value="#00FFFFFF" />
                                        </Trigger>
                                        <Trigger Property="IsEnabled" Value="True">
                                          <Setter Property="Foreground" Value="#7de6fa" />
                                        </Trigger>
                                      </Style.Triggers>
                                    </Style>
                                  </FrameworkElement.Style>
                                </TextBox>
                                <Grid Column="1" HorizontalAlignment="Right" Width="{DynamicResource {x:Static SystemParameters.VerticalScrollBarWidthKey}}">
                                  <Path Name="Arrow" Data="M0,1 C0,1 0,0 0,0 C0,0 3,0 3,0 C3,0 3,1 3,1 C3,1 4,1 4,1 C4,1 4,0 4,0 C4,0 7,0 7,0 C7,0 7,1 7,1 C7,1 6,1 6,1 C6,1 6,2 6,2 C6,2 5,2 5,2 C5,2 5,3 5,3 C5,3 4,3 4,3 C4,3 4,4 4,4 C4,4 3,4 3,4 C3,4 3,3 3,3 C3,3 2,3 2,3 C2,3 2,2 2,2 C2,2 1,2 1,2 C1,2 1,1 1,1 C1,1 0,1 0,1" Fill="#FFFFFFFF" HorizontalAlignment="Center" Margin="3,0" VerticalAlignment="Center" />
                                </Grid>
                              </Grid>
                            </chromes:ButtonChrome>
                            <ControlTemplate.Triggers>
                              <Trigger Property="ToggleButton.IsChecked" Value="True">
                                <Setter TargetName="Chrome" Property="ButtonChrome.RenderPressed" Value="True" />
                              </Trigger>
                              <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="Arrow" Property="Shape.Fill" Value="#FFAFAFAF" />
                              </Trigger>
                              <DataTrigger Value="True" Binding="{Binding IsEditable, RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type toolkit:CheckComboBox}}}">
                                <Setter TargetName="TextBox" Property="TextBoxBase.IsReadOnly" Value="False" />
                                <Setter TargetName="TextBox" Property="Focusable" Value="True" />
                                <Setter TargetName="TextBox" Property="FrameworkElement.Cursor" Value="{x:Null}" />
                                <Setter Property="IsTabStop" Value="True" />
                              </DataTrigger>
                              <MultiDataTrigger>
                                <MultiDataTrigger.Conditions>
                                  <Condition Value="True" Binding="{Binding IsEditable, RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type toolkit:CheckComboBox}}}" />
                                  <Condition Value="True" Binding="{Binding IsFocused, RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type toolkit:CheckComboBox}}}" />
                                </MultiDataTrigger.Conditions>
                                <Setter TargetName="TextBox" Property="FocusManager.FocusedElement" Value="{Binding ElementName=TextBox}" />
                              </MultiDataTrigger>
                            </ControlTemplate.Triggers>
                          </ControlTemplate>
                        </Setter.Value>
                      </Setter>
                    </Style>
                  </FrameworkElement.Style>
                </ToggleButton>
              </Grid>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
        <Style.Triggers>
          <Trigger Property="UIElement.IsMouseOver" Value="True">
            <Setter Property="Control.BorderBrush" Value="{DynamicResource MahApps.Brushes.ComboBox.Border.MouseOver}" />
            <Setter Property="Control.Background" Value="{DynamicResource MahApps.Brushes.Gray8}" />
          </Trigger>
          <Trigger Property="UIElement.IsKeyboardFocusWithin" Value="True">
            <Setter Property="Control.BorderBrush" Value="{DynamicResource MahApps.Brushes.ComboBox.Border.Focus}" />
          </Trigger>
          <Trigger Property="toolkit:CheckComboBox.IsEditable" Value="True">
            <Setter Property="Control.IsTabStop" Value="False" />
          </Trigger>
        </Style.Triggers>
      </Style>
      <Style x:Key="{x:Type primitives:SelectorItem}" xmlns:primitives="clr-namespace:Xceed.Wpf.Toolkit.Primitives;assembly=Xceed.Wpf.Toolkit" TargetType="{x:Type primitives:SelectorItem}">
        <Setter Property="Control.Template">
          <Setter.Value>
            <ControlTemplate TargetType="{x:Type primitives:SelectorItem}">
              <Border Name="_background" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                <CheckBox Padding="{TemplateBinding Padding}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" IsChecked="{Binding IsSelected, RelativeSource={RelativeSource TemplatedParent}}">
                  <ContentControl Content="{TemplateBinding Content}" ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" ContentTemplate="{TemplateBinding ContentTemplate}" Foreground="{TemplateBinding Foreground}" />
                </CheckBox>
              </Border>
              <ControlTemplate.Triggers>
                <Trigger Property="UIElement.IsMouseOver" Value="True">
                  <Setter TargetName="_background" Property="Border.Background" Value="#66AA00FF" />
                </Trigger>
              </ControlTemplate.Triggers>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </Style>
    </ResourceDictionary>
  </FrameworkElement.Resources>
  <Grid>
    <Grid>
      <Grid.RowDefinitions>
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="*" />
      </Grid.RowDefinitions>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <ComboBox Margin="10,12,0,0" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Height="23" xmlns:controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro" controls:TextBoxHelper.Watermark="Battle Mode">
        <ComboBoxItem Content="Fight" ToolTip="Fight automatically in encounters." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.Fight}, Converter={StaticResource EnumToBoolConverter}}" />
        <ComboBoxItem Content="Run" ToolTip="Run automatically in encounters. &#xA;Use this in Combination with Catch Shiny or Anti AFK Shiny for Shiny farming." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.Run}, Converter={StaticResource EnumToBoolConverter}}" />
        <ComboBoxItem Content="Catch" ToolTip="Catch automatically in encounters." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.Catch}, Converter={StaticResource EnumToBoolConverter}}" />
        <ComboBoxItem Content="Pay Day" Foreground="#7de6fa" ToolTip="Uses Pay Day against Pokemon to farm money!" IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.PayDay}, Converter={StaticResource EnumToBoolConverter}}" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Pay Day + Catch [MIXED]" Foreground="#7de6fa" ToolTip="Uses Pay Day or Catch depending on your settings!" IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.PayDayCatchMixed}, Converter={StaticResource EnumToBoolConverter}}" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Pay Day + Thief [MIXED]" Foreground="#7de6fa" ToolTip="Uses Pay Day or Thief depending on your settings!" IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.PayDayThiefMixed}, Converter={StaticResource EnumToBoolConverter}}" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Thief" Foreground="#7de6fa" ToolTip="Uses Thief against Pokemon to steal their item for money!" IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.Thief}, Converter={StaticResource EnumToBoolConverter}}" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Pickpocket" Foreground="#7de6fa" ToolTip="Steals items from horde Pokemon using Imprison + Pickpocket ability!&#xA;Pokemon 1: Frisk ability + Imprison (Move Slot 2).&#xA;Pokemon 2: Pickpocket ability." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.Pickpocket}, Converter={StaticResource EnumToBoolConverter}}" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Safari" Foreground="#7de6fa" ToolTip="Catch automatically in Safari encounters." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.Safari}, Converter={StaticResource EnumToBoolConverter}}" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Sell Box" ToolTip="Sells automatically all Pokemon in the current Box." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.SellBox}, Converter={StaticResource EnumToBoolConverter}}" />
        <ComboBoxItem Content="Mail Claim" ToolTip="Claim automatically all mails in the PC." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.MailClaim}, Converter={StaticResource EnumToBoolConverter}}" />
        <ComboBoxItem Content="GTL Sniper" Foreground="#7de6fa" ToolTip="GTL Sniper" IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.GTLSniper}, Converter={StaticResource EnumToBoolConverter}}" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="None" ToolTip="Does nothing in encounters so you can battle manually." IsSelected="{Binding BotMode, ConverterParameter={x:Static model:BotMode.None}, Converter={StaticResource EnumToBoolConverter}}" />
      </ComboBox>
      <CheckBox Margin="10,10,0,0" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" Content="Level 1st" ToolTip="Will instantly switch to the second pokemon in battles to level up the first one." Visibility="{Binding FightOptionVisibility}" IsEnabled="{Binding LevelFirstEnabled}" IsChecked="{Binding LevelFirst}" />
      <StackPanel Margin="1,10,0,0" Grid.Row="0" Grid.Column="3" Grid.ColumnSpan="2" Orientation="Horizontal" Visibility="{Binding CatchSpellsVisbility}">
        <Button Grid.Row="0" Grid.Column="3" Background="#00FFFFFF" BorderThickness="0" ToolTip="Choose Poke Ball" Command="{Binding ChoosePokeBallCommand}">
          <ContentControl.Content>
            <iconpacks:PackIconMaterial FrameworkElement.Width="18" FrameworkElement.Height="18" Control.Foreground="{DynamicResource MahApps.Brushes.AccentBase}" xmlns:iconpacks="clr-namespace:MahApps.Metro.IconPacks;assembly=MahApps.Metro.IconPacks.Material" iconpacks:PackIconMaterial.Kind="Pokeball" />
          </ContentControl.Content>
        </Button>
        <CheckBox Margin="8,0,0,0" Content="Switch" ToolTip="Will instantly switch to the second pokemon in battles to catch with." IsChecked="{Binding CatchWithSecondPokemon}" />
      </StackPanel>
      <StackPanel Orientation="Horizontal" Margin="10,12,0,0" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Visibility="{Binding CatchPokemonVisibility}">
        <ComboBox Width="100" Height="23" p15:TextBoxHelper.Watermark="Pokemon" Text="{Binding CatchPokemon}" ItemsSource="{converter:EnumBindingSource {x:Type model:Pokemon}}" SelectedIndex="{Binding CatchPokemonSelectedIndex}" xmlns:p15="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro" />
        <Button Margin="1,0,0,0" Height="28" ToolTip="Take Screenshot" Background="#00FFFFFF" BorderThickness="0" xmlns:controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro" controls:ControlsHelper.FocusBorderThickness="0" Command="{Binding ScreenshotPokemonNameCommand}">
          <ContentControl.Content>
            <PackIconMaterial FrameworkElement.Width="16" FrameworkElement.Height="16" Control.Foreground="{DynamicResource MahApps.Brushes.AccentBase}" p18:PackIconMaterial.Kind="MonitorScreenshot" xmlns:p18="clr-namespace:MahApps.Metro.IconPacks;assembly=MahApps.Metro.IconPacks.Material" xmlns="clr-namespace:MahApps.Metro.IconPacks;assembly=MahApps.Metro.IconPacks.Material" />
          </ContentControl.Content>
        </Button>
      </StackPanel>
      <TextBox Margin="10,12,0,0" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" p14:TextBoxHelper.Watermark="Sell Price" Visibility="{Binding SellBoxVisibility}" Text="{Binding SellPrice}" xmlns:p14="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro" />
      <CheckBox Margin="10,10,0,0" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" Content="IV 31" CheckBoxHelper.ForegroundUnchecked="#64fb64" CheckBoxHelper.ForegroundUncheckedMouseOver="#64fb64" CheckBoxHelper.ForegroundChecked="#64fb64" CheckBoxHelper.ForegroundCheckedMouseOver="#64fb64" CheckBoxHelper.ForegroundCheckedPressed="#64fb64" CheckBoxHelper.ForegroundUncheckedPressed="#64fb64" ToolTip="Will release pokemon that doesn't have at least 1 IV 31.&#xA;The bot will only check pokemon that you've caught and it will not release any shinies.&#xA;The stats window must be in default position (center of your screen), to reset the position (if you moved it) restart PokeMMO." Visibility="{Binding IV31Visibility}" IsChecked="{Binding OnlyKeepIV31}" />
      <CheckComboBox FrameworkElement.Margin="5,12,10,0" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" FrameworkElement.Height="23" Control.Foreground="#FFFFFFFF" p14:CheckComboBox.Text="Catch Moves" ItemsControl.DisplayMemberPath="CatchSpells" xmlns:primitives="clr-namespace:Xceed.Wpf.Toolkit.Primitives;assembly=Xceed.Wpf.Toolkit" primitives:Selector.SelectedMemberPath="Selected" FrameworkElement.Style="{DynamicResource CheckComboBoxStyle1}" ItemsControl.ItemsSource="{Binding Options}" UIElement.Visibility="{Binding CatchSpellsVisbility}" UIElement.IsEnabled="{Binding PremiumEnabled}" xmlns:p14="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit" xmlns="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit" />
      <CheckBox Margin="10,10,0,0" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Content="2+ Pay Day" ToolTip="If you check the checkbox it will switch to the next Pay Day Pokemon when the PP hits 0. (for Auto Walk/Fish)&#xA;For Auto SS, its active by default, since you walk back when PP of SS hits 0." Visibility="{Binding PayDayOptionVisibility}" IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding MorePayDay}" />
      <CheckBox Margin="10,10,0,0" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="3" Content="Multi Target Move (2)" ToolTip="Will automatically use Move 2 after using Pay Day (for example a multi target move to clear up hordes easier)." Visibility="{Binding PayDayOptionVisibility}" IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding PayDayMultiTarget}" />
      <StackPanel Orientation="Horizontal" Margin="10,10,0,0" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="3">
        <CheckBox Content="2+ Thief" ToolTip="If you check the checkbox it will switch to the second Thief Pokemon in order to get the second item.&#xA;This option is slower but it makes sure you don't miss any items." Visibility="{Binding ThiefOptionVisibility}" IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding MoreThief}" />
        <CheckBox Margin="21,0,0,0" Content="Imprison" ToolTip="Will automatically use Imprison before using Thief.&#xA;Imprison has to be placed in Move Slot 2." Visibility="{Binding ThiefOptionVisibility}" IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding Imprison}" />
      </StackPanel>
      <StackPanel Orientation="Horizontal" Margin="10,10,0,0" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2">
        <CheckBox Content="Rock" ToolTip="Will automatically Throw a Rock before trying to catch the Pokemon." Visibility="{Binding SafariOptionVisibility}" IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding Rock}" Checked="chk_rock_Checked" />
        <CheckBox Margin="18,0,0,0" Content="Bait" ToolTip="Will automtically Throw a Bait before trying to catch the Pokemon." Visibility="{Binding SafariOptionVisibility}" IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding Bait}" Checked="chk_bait_Checked" />
      </StackPanel>
      <GridSplitter IsEnabled="False" Margin="10,10,10,0" Height="2" Grid.Row="2" Grid.ColumnSpan="5" />
      <CheckBox Name="chk_walk" Margin="10,10,0,0" Grid.Row="3" Grid.Column="0" Content="Walk" ToolTip="Walk automatically.&#xA;You can change the Walk Direction from Left-Right, Up-Down, Squares and Random." IsChecked="{Binding Walk}" Checked="chk_walk_Checked" />
      <CheckBox Name="chk_fish" Margin="10,10,0,0" Grid.Row="3" Grid.Column="1" Content="Fish" ToolTip="Fish automatically.&#xA;Rod needs to be placed on Hotkey Slot 1 in the Action Bar." IsChecked="{Binding Fish}" Checked="chk_fish_Checked" />
      <StackPanel Orientation="Horizontal" Margin="10,10,0,0" Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="3">
        <CheckBox Name="chk_auto_walk" Content="Auto Walk/Fish" ToolTip="Walk/Fish automatically until you run out of any of your PP and automatically restore PP.&#xA;Will use the selected 'Automatic Walk Route' to walk to the spot." IsChecked="{Binding AutoWalkFish}" Visibility="{Binding AutoWalkFishVisibility}" Checked="chk_auto_walk_Checked" />
        <CheckBox Name="chk_auto_sweet_cent" Margin="10,0,0,0" Content="Auto SS" ToolTip="Use Sweet Scent automatically until you run out of PP and automatically restore PP.&#xA;Will use the selected 'Automatic Sweet Scent Route' to walk to the spot." IsChecked="{Binding AutoSweetScent}" Visibility="{Binding AutoSweetScentVisibility}" Checked="chk_auto_sweet_cent_Checked" />
      </StackPanel>
      <StackPanel Orientation="Horizontal" Margin="10,10,0,0" Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="3">
        <CheckBox Content="Auto Walk" ToolTip="Will use the 'Automatic Walk Route' for Safari to walk to the spot and start to walk." IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding SafariAutoWalk}" Visibility="{Binding SafariOptionVisibility}" Checked="chk_safari_auto_walk_Checked" />
        <CheckBox Margin="10,0,0,0" Content="Auto Fish" ToolTip="Will use the 'Automatic Fish Route' for Safari to walk to the spot and start to fish." IsEnabled="{Binding PremiumEnabled}" IsChecked="{Binding SafariAutoFish}" Visibility="{Binding SafariOptionVisibility}" Checked="chk_safari_auto_fish_Checked" />
      </StackPanel>
      <controls:ToggleSwitch FrameworkElement.Margin="10,5,0,0" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" xmlns:controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro" controls:ToggleSwitch.OnContent="Left Right" controls:ToggleSwitch.OffContent="Up Down" controls:ToggleSwitch.IsOn="{Binding WalkDirection}" />
      <StackPanel Orientation="Horizontal" Margin="10,5,0,0" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="3" Visibility="{Binding Walk, Converter={StaticResource BoolToVis}}">
        <CheckBox Content="Squares" IsChecked="{Binding SquaresWalkPattern}" Checked="chk_squareswalkpattern_Checked" />
        <CheckBox Margin="23,0,0,0" Content="Random" IsChecked="{Binding RandomWalkPattern}" Checked="chk_randomwalkpattern_Checked" />
      </StackPanel>
      <ComboBox Margin="10,5,10,0" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="3" Height="23" p14:TextBoxHelper.Watermark="Automatic Sweet Scent Routes" FontSize="10" SelectedValuePath="Content" SelectedValue="{Binding SelectedAutoSweetScentRoute}" Visibility="{Binding ShowAutoSweetScentRoutes, Converter={StaticResource BoolToVis}}" xmlns:p14="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro">
        <ComboBoxItem Content="Kanto_Route_4" />
        <ComboBoxItem Content="Unova_Mistralton_City_1" />
        <ComboBoxItem Content="Kanto_Celadon_City" Foreground="#7de6fa" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Kanto_Cerulean_City" Foreground="#7de6fa" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Kanto_Cinnabar_Island" Foreground="#7de6fa" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Kanto_Fuchsia_City" Foreground="#7de6fa" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Kanto_Indigo_Plateau" Foreground="#7de6fa" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Kanto_Island_One" Foreground="#7de6fa" IsEnabled="{Binding PremiumEnabled}" />
        <ComboBoxItem Content="Kanto_Island_Six_1" Foreground="#7de6fa" IsEnabled="{Binding PremiumEnabled}" />
